{
    "ecmaFeatures": {
        "objectLiteralShorthandMethods": true,
        "objectLiteralShorhandProperties": true,
        "blockBindings": true,
        "forOf": true,
        "spread": true,
        "destructuring": true,
        "jsx": true,
        "destructuring": true,
        "arrowFunctions": true,
        "defaultParams": true
    },
    "globals":{
      "window": true,
      "require": true,
      "document": true,
      "module": true,
      "setInterval": true,
      "setTimeout": true,
      "clearInterval": true
    },
    "plugins":[
      "react"
    ],
    "rules": {
        "react/jsx-boolean-value": 1,
        "react/jsx-quotes": 1,
        "react/jsx-no-undef": 1,
        "react/jsx-uses-react": 1,
        "react/jsx-uses-vars": 1,
        "react/no-did-mount-set-state": 1,
        "react/no-did-update-set-state": 1,
        "react/no-multi-comp": 0,
        "react/no-unknown-property": 1,
        "react/prop-types": 1,
        "react/react-in-jsx-scope": 1,
        "react/self-closing-comp": 1,
        "react/wrap-multilines": 1,
        "quotes": [1,"single"],
        "semi": 2,
        "strict": 0,
        "camelcase": 0,
        "no-var": 1,
        "no-underscore-dangle": 0
    }
}
